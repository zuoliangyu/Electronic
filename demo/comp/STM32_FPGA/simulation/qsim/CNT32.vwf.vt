module CNT32_vlg_vec_tst();
// constants                                           
// general purpose registers
reg CLK;
reg CLKBASE;
reg CLKBASEEN;
reg CLKEN;
reg CLR;
// wires                                               
wire [31:0] Q;
wire [31:0] QBASE;

wire sampler;                             

// assign statements (if any)                          
CNT32 i1 (
// port map - connection between master ports and signals/registers   
	.CLK(CLK),
	.CLKBASE(CLKBASE),
	.CLKBASEEN(CLKBASEEN),
	.CLKEN(CLKEN),
	.CLR(CLR),
	.Q(Q),
	.QBASE(QBASE)
);

// CLK
initial
begin
	repeat(150)
	begin
		CLK = 1'b0;
		CLK = #3333 1'b1;
		# 3333;
	end
	CLK = 1'b0;
end 

// CLKBASE
always
begin
	CLKBASE = 1'b0;
	CLKBASE = #5000 1'b1;
	#5000;
end 

// CLKBASEEN
initial
begin
	CLKBASEEN = 1'b1;
	CLKBASEEN = #70000 1'b0;
	CLKBASEEN = #60000 1'b1;
end 

// CLKEN
initial
begin
	CLKEN = 1'b1;
	CLKEN = #70000 1'b0;
	CLKEN = #60000 1'b1;
end 

// CLR
initial
begin
	CLR = 1'b1;
	CLR = #150000 1'b0;
	CLR = #30000 1'b1;
end 

CNT32_vlg_sample_tst tb_sample (
	.CLK(CLK),
	.CLKBASE(CLKBASE),
	.CLKBASEEN(CLKBASEEN),
	.CLKEN(CLKEN),
	.CLR(CLR),
	.sampler_tx(sampler)
);

CNT32_vlg_check_tst tb_out(
	.Q(Q),
	.QBASE(QBASE),
	.sampler_rx(sampler)
);
endmodule

