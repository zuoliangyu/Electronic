module CNT32_vlg_sample_tst(
	CLK,
	CLKBASE,
	CLKBASEEN,
	CLKEN,
	CLR,
	sampler_tx
);
input  CLK;
input  CLKBASE;
input  CLKBASEEN;
input  CLKEN;
input  CLR;
output sampler_tx;

reg sample;
time current_time;
always @(CLK or CLKBASE or CLKBASEEN or CLKEN or CLR)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

